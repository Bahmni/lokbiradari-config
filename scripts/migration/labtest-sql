#remove first 8 results from the below query result and add respective datatypes manually

select '' as "uuid", 
	"column_name" as "name", 
	'' as "description", 
	'LabTest' as "class",
	'' as "datatype",
	'' as "units", 
	'' as "shortname", 
	'' as "synonym.1", 
	'' as "reference-term-source", 
	'' as "reference-term-code", 
	'' as "reference-term-relationship",
	'' as "answer.1"
	from information_schema.columns where
	table_name='investigationmaster';
